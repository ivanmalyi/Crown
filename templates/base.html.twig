        <script src="{{ asset("asset/js/color.js") }}"></script>
        <script src="{{ asset("asset/js/product.js") }}"></script>
        <script src="{{ asset("asset/js/auth.js") }}"></script>
        <script src="{{ asset("asset/js/img.js") }}"></script>
        <script src="{{ asset("asset/js/mainPage.js") }}"></script>
    {% endblock %}
